{
  "name": "linqts",
  "version": "1.5.0",
  "description": "LinQ + TypeScript",
  "main": "dist/linq.js",
  "typescript": {
    "definition": "dist/linq.d.ts"
  },
  "scripts": {
    "start": "tsc -w",
