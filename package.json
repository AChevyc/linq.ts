  },
  "license": "MIT",
  "typings": "linq.d.ts",
  "bugs": {
    "url": "https://github.com/kutyel/linq.ts/issues"
  },
  "homepage": "https://github.com/kutyel/linq.ts#readme",
  "dependencies": {
    "typescript": "^1.8.10"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "coveralls": "^2.11.11",
    "nyc": "^7.0.0",
    "tslint": "^3.13.0",
    "typedoc": "^0.4.4"
  }
